2)import math
a = int(input('a = '))
b = int(input('b = '))
c = math.sqrt(a**2+b**2)
P = a + b + c
print('P = ', P)



3)A = int(input())
B = int(input())
C = A
A = B
B = C
print('A = ', A)
print('B = ', B)



4)N = int(input("N = "))
if 99<N<1000:
    d1 = N%10
    N = N//10
    d10 = N%10
    N = N//10
    d100 = N%10
    T = d1+d10+d100
    print('T = ', T)
else:
    print('Net')
